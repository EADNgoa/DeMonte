@model PagedList.IPagedList<DeMonte.BillDetail>
@using PagedList.Mvc;

@{
    ViewBag.Title = "BillDetailS";
}

<center>
    <h2 class="section-to-print"><b>@ViewBag.Title</b></h2>
    
   
</center>

<table class="table">
    <tr>
       

        <th>Date</th>
       <th>Extra Person</th>
        <th>GST</th>
        <th>Miscelleneous</th>
        <th>Other 1</th>
        <th>Other 2</th>
        <th>Other 3</th>
        <th>Other 4</th>
        <th>Total</th>
      
       

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ExtraPerson)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GST)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Miscelleneous)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Other1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Other2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Other3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Other4)
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.Total)
         </td>
       
       
        <td>
            @Html.ActionLink("Edit", "Manage", new { id = item.BillDetailID})
        </td>
      
        
    </tr>
}

</table>
